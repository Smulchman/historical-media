<div class="container" id="main">
  <div class="card">
    <div class="card-header" id="year">
      Year:{{year}}
    </div>
    <div class="card-body">
      <p class="card-text">{{description}}</p>
      <button
        type="button"
        class="btn btn-outline-success btn-sm blog"
        data-eventId={{id}}
      >Blog about this event!</button>
    </div>
    <div>
      <a
        id="link"
        href="#"
        class="btn btn-sm btn-link event"
        data-eventId={{id}}
      >More links about this event!</a>
    </div>

  </div>
</div>